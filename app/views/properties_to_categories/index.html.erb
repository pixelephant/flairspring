<h1>Listing properties_to_categories</h1>

<table>
  <tr>
    <th>Property</th>
    <th>Category</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @properties_to_categories.each do |properties_to_category| %>
  <tr>
    <td><%= properties_to_category.property_id %></td>
    <td><%= properties_to_category.category_id %></td>
    <td><%= link_to 'Show', properties_to_category %></td>
    <td><%= link_to 'Edit', edit_properties_to_category_path(properties_to_category) %></td>
