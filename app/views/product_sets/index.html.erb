<h1>Listing product_sets</h1>

<table>
  <tr>
    <th>Price</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_sets.each do |product_set| %>
  <tr>
    <td><%= product_set.price %></td>
    <td><%= product_set.description %></td>
    <td><%= link_to 'Show', product_set %></td>
    <td><%= link_to 'Edit', edit_product_set_path(product_set) %></td>
    <td><%= link_to 'Destroy', product_set, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Product set', new_product_set_path %>
