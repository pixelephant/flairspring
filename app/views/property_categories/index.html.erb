<h1>Listing property_categories</h1>

<table>
  <tr>
    <th>Category name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @property_categories.each do |property_category| %>
  <tr>
    <td><%= property_category.category_name %></td>
    <td><%= link_to 'Show', property_category %></td>
    <td><%= link_to 'Edit', edit_property_category_path(property_category) %></td>
    <td><%= link_to 'Destroy', property_category, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Property category', new_property_category_path %>
