<h1>Listing products_stores</h1>

<table>
  <tr>
    <th>Store</th>
    <th>Product</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @products_stores.each do |products_store| %>
  <tr>
    <td><%= products_store.store_id %></td>
    <td><%= products_store.product_id %></td>
    <td><%= link_to 'Show', products_store %></td>
    <td><%= link_to 'Edit', edit_products_store_path(products_store) %></td>
    <td><%= link_to 'Destroy', products_store, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

